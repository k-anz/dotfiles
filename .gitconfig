[user]
	email = 
	name = Kyoko OZAKI
[push]
	default = nothing
[credential]
        helper = cache
[alias]
        st = status
        cm = commit
        co = checkout
        b = branch
        cp = cherry-pick
        gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
        df = diff
        dfw = diff --word-diff
        dfn = diff --name-only
        dfs = diff --stat
        dfc = diff --cached
[color]
        ui = auto
[init]
        templatedir = ~/.git_template
[core]
	editor = nvim
